{
    "name": "meirelles/enum-mapper",
    "description": "Bridge your static tables to PHP enums seamlessly.",
    "version": "0.0.0",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "Meirelles\\EnumMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Luis Meirelles",
            "email": "luis.gustavo.souto.meirelles@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.3",
        "ext-pdo": "*",
        "symfony/dotenv": "^7.1",
        "mustache/mustache": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "symfony/var-dumper": "^7.1"
    }
}
